{
  "name": "wavevision/latte-filters",
  "description": "Extra filters for Latte template engine.",
  "type": "library",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Wavevision s.r.o.",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "nette/http": "^3.0",
    "wavevision/utils": "^2.1"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.1",
    "wavevision/coding-standard": "^6.0",
    "wavevision/di-service-annotation": "^4.0",
    "wavevision/nette-tests": "^3.1"
  }
}
